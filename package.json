{
	"name": "berserk",
	"version": "1.0.0",
	"description": "Berserk Framework node",
	"main": "src/index.js",
	"scripts": {
		"berserk": "nodemon index.js",
		"dev": "NODE_ENV=development  concurrently \"yarn run berserk\"",
		"preinstall": "which link-module-alias && link-module-alias clean || echo",
		"postinstall": "link-module-alias"
	},
	"_moduleAliases": {
		"~root": ".",
		"controller": "src/app/controllers",
		"public": "src/public",
		"config": "src/config.js",
		"Api": "src/routes/api.js",
		"Web": "src/routes/web.js"
	},
	"author": "Fusengine <info@fusengine.ch>",
	"license": "MIT",
	"dependencies": {
		"@fusengine/berserk-engine": "^1.0.25",
		"concurrently": "^4.1.2",
		"ejs": "^2.6.2",
		"ejs-mate": "^3.0.0",
		"express": "^4.17.1",
		"express-validator": "^6.1.1",
		"link-module-alias": "^1.2.0",
		"mongoose": "^5.6.9",
		"nodemon": "^1.19.1",
		"pug": "^2.0.4"
	}
}
