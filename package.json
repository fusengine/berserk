{
	"name": "berserk",
	"version": "1.5.0",
	"description": "Berserk Framework node from fusengine",
	"main": "src/index.js",
	"scripts": {
		"berserk": "nodemon index.js",
		"dev": "NODE_ENV=development  concurrently \"yarn run berserk\"",
		"prod": "NODE_ENV=production  concurrently \"yarn run berserk\"",
		"preinstall": "which link-module-alias && link-module-alias clean || echo",
		"postinstall": "link-module-alias"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/fusengine/berserk"
	},
	"files": [
		"LICENCE",
		"README.md",
		"src/index.js",
		"src/"
	],
	"engines": {
		"node": ">= 8.9.0"
	},
	"_moduleAliases": {
		"~root": ".",
		"Berserk": "src/app",
		"Config": "src/config",
		"Public": "src/public",
		"Route": "src/routes"
	},
	"author": "Fusengine <info@fusengine.ch>",
	"license": "MIT",
	"dependencies": {
		"@fusengine/berserk-engine": "^1.4.0",
		"bcrypt": "^3.0.6",
		"concurrently": "^5.0.0",
		"connect-mongo": "^3.0.0",
		"ejs": "^2.7.1",
		"ejs-mate": "^3.0.0",
		"express": "^4.17.1",
		"express-validator": "^6.2.0",
		"link-module-alias": "^1.2.0",
		"mongoose": "^5.7.5",
		"nodemon": "^1.19.3",
		"passport": "^0.4.0",
		"passport-local": "^1.0.0",
		"pug": "^2.0.4"
	}
}