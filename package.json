{
	"name": "berserk",
	"version": "1.4.2",
	"description": "Berserk Framework node from fusengine",
	"author": "Fusengine <info@fusengine.ch>",
	"license": "MIT",
	"main": "src/config/berserk.start.js",
	"scripts": {
		"berserk": "nodemon src/config/start.js",
		"dev": "NODE_ENV=development  concurrently \"yarn run berserk\"",
		"prod": "NODE_ENV=production  concurrently \"yarn run berserk\"",
		"preinstall": "which link-module-alias && link-module-alias clean || echo",
		"postinstall": "link-module-alias"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/fusengine/berserk"
	},
	"files": [
		"LICENCE",
		"README.md",
		"src/index.js",
		"src/"
	],
	"engines": {
		"node": ">= 10.*"
	},
	"_moduleAliases": {
		"~root": ".",
		"Berserk": "src/app",
		"Config": "src/config",
		"Controller": "src/app/controllers",
		"Database": "src/database",
		"ENV_FILE": "src/env.js",
		"Public": "src/public",
		"Route": "src/routes",
		"Views": "src/views"
	},
	"dependencies": {
		"@fusengine/berserk-engine": "^1.8.3",
		"bcrypt": "^4.0.1",
		"concurrently": "^5.1.0",
		"connect-mongo": "^3.2.0",
		"cookie-parser": "^1.4.5",
		"ejs": "^3.0.2",
		"ejs-mate": "^3.0.0",
		"express": "^4.17.1",
		"express-session": "^1.17.0",
		"express-validator": "^6.4.0",
		"jsonwebtoken": "^8.5.1",
		"link-module-alias": "^1.2.0",
		"moment": "^2.24.0",
		"mongoose": "^5.9.7",
		"multer": "^1.4.2",
		"nodemailer": "^6.4.6",
		"nodemon": "^2.0.3",
		"passport": "^0.4.1",
		"passport-local": "^1.0.0",
		"pug": "^2.0.4",
		"uuid": "^7.0.3"
	}
}